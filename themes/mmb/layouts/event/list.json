[
    {{ range $index, $e := (where .Data.Pages "Type" "event" ) }}
    {{ if $index }}, {{ end }}
    {
        "title": "{{ .Title }}",
        "start": "{{ .Params.startDate }}",
        "end": "{{ .Params.endDate }}",
        "allDay": {{ .Params.allday }},
        "url": "{{ .URL }}",
        "extendedProps": {
            {{ with .Params.location }}"location": "{{ . }}",{{ end }}
            {{ with .Params.category }}"category": "{{ . }}",{{ end }}
            "description": "{{ trim (.Content | plainify) "\n" }}"
        }
    }
    {{ end }}
]