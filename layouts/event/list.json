[
    {{ range $index, $e := (where .Data.Pages "Type" "event" ) }}
    {{ if $index }}, {{ end }}
    {
        "title": "{{ .Title }}",
        "start": "{{ .Params.startDate }}",
        "end": "{{ .Params.endDate }}",
        "allDay": {{ .Params.allday }},
        "url": "{{ .Permalink | absURL }}",
        "extendedProps": {
            "location": "{{ .Params.location }}",
            "description": "{{ trim (.Content | plainify) "\n" }}"
        }
    }
    {{ end }}
]